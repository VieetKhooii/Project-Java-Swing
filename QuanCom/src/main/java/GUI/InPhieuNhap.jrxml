	<queryString language="SQL">
		<![CDATA[SELECT pn.phieu_id, nv.name, sup.sup_name, pn.date, pn.tonggia,ctpn.material_id, ctpn.name, ctpn.soluong, ctpn.gia
FROM phieunhap pn
